page{
    width: 100%;
    height: 100%;
    display: flex;
    flex-direction: column;
}
.body{
    width:750rpx;
    height:100%;
    flex: 1;
    background:rgba(246,247,249,1);
    /*border-radius:4rpx;*/

    display: flex;
    flex-direction: column;


}
.item-gray{
    font-size:28rpx;
    font-family:PingFangSC-Regular;
    font-weight:400;
    color:rgba(34,34,34,1);
    line-height:40rpx;

    box-sizing: border-box;
    padding-left: 30rpx;
    padding-right: 30rpx;
    padding-bottom: 24rpx;
    width: 100%;
}
.w-c{
    background:rgba(255,255,255,1);

    box-sizing: border-box;
    width: 100%;
    padding-left: 30rpx;
    padding-right: 40rpx;
}
.item-c{
    box-sizing: border-box;
    display: flex;
    flex-direction: row;
    justify-content: space-between;
    width: 100%;
    padding: 24rpx 0;

}
.item-c-l{
    font-size:28rpx;
    font-family:PingFangSC-Regular;
    font-weight:400;
    color:rgba(136,136,136,1);
    line-height:40rpx;

}
.phone{
    font-size:28rpx;
    font-family:PingFangSC-Regular;
    font-weight:400;
    color:rgba(64,158,255,1);
    line-height:40rpx;

}
.item-c-r{
    font-size:28rpx;
    font-family:PingFangSC;
    font-weight:500;
    color:rgba(34,34,34,1);
    line-height:40rpx;
}
.dialog{
    width:560rpx !important;
    height:236rpx !important;
    background:rgba(255,255,255,1) !important;
    border-radius:16rpx !important;
    filter:blur(0px) !important;
}
.dialog-title{
    width: 100%;
    height: 146rpx;

    display: flex;
    align-items: center;
    justify-content: center;

    font-size:32rpx;
    font-family:PingFangSC-Regular;
    font-weight:400;
    color:rgba(0,0,0,1);
    line-height:44rpx;

    border-bottom: 1rpx rgba(238,238,238,1) solid;
}
.dialog-action{
    width: 100%;
    height: 90rpx;
    display: flex;

    position: absolute;
    bottom: 0;
}
.dialog-button{
    flex: 1;
    width: 100%;
    height: 100%;

    display: flex;
    align-items: center;
    justify-content: center;


    font-size:28rpx;
    font-family:PingFangSC-Regular;
    font-weight:400;
    color:rgba(64,158,255,1);
    line-height:40rpx;

    border-right: 1rpx rgba(238,238,238,1) solid;
}