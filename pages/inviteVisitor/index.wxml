<image class="img-bg" src="/images/invite_visitor_bg.png"/>
<cu-custom  isBack="{{false}}">
    <view slot="backText"></view>
    <view slot="content"><text class="{{navigationBarLoading?'wj-loading-icon':''}}"/></view>
</cu-custom>
<view class="body">

    <image class="image-wj m-t-50" src="/images/image_wj.png" />
    <view class="logo-text">吾 家</view>
    <view class="container-t">
        <view class="c-name">{{apiData.communityName}}</view>
        <view class="ndsyms">输入<text class="six-bit">六位</text>邀请码开锁</view>
        <view class="flex code-container" >
            <text class="code-text asterisk">*</text>
            <text wx:for="{{apiData.applyCodeSplit}}" class="code-text">{{item}}</text>
            <text class="code-text asterisk">*</text>
        </view>
        <view class="btn-copy" hover-class="navigator-hover" hover-stop-propagation="{{true}}" catchtap="copyCode" >复制受邀码</view>
        <view class="flex align-center w-p-100 m-t-30">
            <view class="l-corner"></view>
            <view class="dash-line-y flex-1"></view>
            <view class="r-corner"></view>
        </view>
<!--        <view class="b-text">输入六位邀请码开锁</view>-->
        <view class="b-text p-t-20 p-b-50">邀请码截止时间：{{apiData.endDate}}</view>

    </view>

</view>